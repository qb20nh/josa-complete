  "description": "한국어 조사를 완성시켜줍니다",
  "main": "./dist/index.js",
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "eslint": "^8.32.0",
